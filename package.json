    "@oslojs/crypto": "1.0.1",
    "@oslojs/encoding": "1.1.0",
    "@prisma/client": "6.4.1",
    "@radix-ui/react-checkbox": "1.1.4",
    "@radix-ui/react-label": "2.1.2",
    "@react-email/components": "0.0.33",
    "@react-router/node": "7.2.0",
    "@react-router/serve": "7.2.0",
    "bcryptjs": "3.0.2",
